&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>$ git fetch --no-tags origin refs/heads/master
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>From https://github.com/github/octocat
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span> * branch master -&amp;gt; FETCH_HEAD
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span> * &lt;span style="color:#ce5c00;font-weight:bold">[&lt;/span>new branch&lt;span style="color:#ce5c00;font-weight:bold">]&lt;/span> master -&amp;gt; origin/master
